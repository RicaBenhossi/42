{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: clang-10 build active file",
            "command": "/usr/bin/clang",
            "args": [
                "-g",
				"-lbsd",
                "${file}",
				"-L.",
				"-lft",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}.out"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}
